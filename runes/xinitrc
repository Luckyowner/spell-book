#!/usr/bin/env sh
xrdb -merge ~/.config/X11/Xresources

# deamons
changeMeWallCicle &
clean_ws_names &
picom --experimental-backends --daemon
battery_check &
dunst &
clipmenud &
command -V espanso && espanso -v worker &

# Settings
k # keyboard settings
if command -V herbstluftwm >/dev/null 2>/dev/null; then
    exec herbstluftwm
else
    exec bspwm
fi
